DEPLOY_SETTINGS="-s /home/tom/.m2/settings.xml"
J_ARTIFACT="mvn $DEPLOY_SETTINGS deploy:deploy-file -DrepositoryId=jboss-releases-repository -Durl=https://repository.jboss.org/nexus/service/local/staging/deploy/maven2"

#cp ~/.m2/repository/apache-log4cxx/contrib/log4cxx/902683/log4cxx-902683.pom . && gedit *.pom && jar cf log4cxx-902683-sources.jar log*pom && jar cf log4cxx-902683.jar log*pom && $J_ARTIFACT -DpomFile=log4cxx-902683.pom -Dversion=902683 -Dfile=log4cxx-902683.jar -Dsources=log4cxx-902683-sources.jar && rm *pom
#cp ~/.m2/repository/apr-1/contrib/apr-1/1.3.3/apr-1-1.3.3.pom . && gedit *.pom && jar cf apr-1-1.3.3-sources.jar apr*pom && jar cf apr-1-1.3.3.jar apr*pom && $J_ARTIFACT -DpomFile=apr-1-1.3.3.pom -Dverion=1-1.3.3 -Dfile=apr-1-1.3.3.jar -Dsources=apr-1-1.3.3-sources.jar && rm *pom

CLASSIFIER=fc18x64
C_ARTIFACT="$J_ARTIFACT -Dclassifier=$CLASSIFIER"

cp ~/.m2/repository/apr-1/contrib/apr-1/1.3.3/apr-1-1.3.3.pom . && gedit *.pom && cp ~/.m2/repository/apr-1/contrib/apr-1/1.3.3/apr-1-1.3.3-$CLASSIFIER.jar . && jar cf apr-1-1.3.3-sources.jar apr*pom && $C_ARTIFACT -DpomFile=apr-1-1.3.3.pom -Dverion=1-1.3.3 -Dfile=apr-1-1.3.3-$CLASSIFIER.jar -Dsources=apr-1-1.3.3-sources.jar && rm *pom *.zip *.jar
exit
